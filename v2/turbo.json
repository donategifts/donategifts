{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": ["tsconfig.json"],
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
        },
        "lint": {},
        "dev": {
            "dependsOn": ["^dev"],
            "cache": false,
            "persistent": true
        }
    }
}
