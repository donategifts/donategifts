{
  "name": "donategifts",
  "description": "Every child should feel valued, remembered, and loved Donate holiday & birthday gifts to the kids who may not otherwise get one ",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./node_modules ./dist",
    "coverage": "jest --runInBand --coverage",
    "dev": "nodemon",
    "husky": "npx husky install",
    "lint": "eslint --cache --fix --ext .ts --ignore-path .eslintignore src/",
    "postinstall": "npm run prisma:generate && npm run husky",
    "prisma:generate": "npx prisma generate",
    "prisma:introspect": "npx prisma introspect",
    "prisma:migrate-dev": "npx prisma migrate dev --name ",
    "start": "node dist/index.js",
    "test-integration:watch": "jest --runInBand --watch",
    "test-integration": "jest --runInBand",
    "test:watch": "jest --testPathIgnorePatterns test/integration --watch",
    "test": "jest --testPathIgnorePatterns test/integration",
    "upgradeDeps": "npx ncu -u"
  },
  "author": "DonateGifts Inc.",
  "license": "NPOSL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donategifts/donategifts.git"
  },
  "dependencies": {
    "@prisma/client": "^2.25.0",
    "@types/express": "^4.17.12",
    "apollo-server-express": "^2.25.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "nodemailer": "^6.6.2",
    "nodemailer-mailgun-transport": "^2.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^15.12.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "codecov": "^3.8.2",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^33.0.1",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "nodemon": "^2.0.7",
    "npm-check-updates": "^11.6.0",
    "prettier": "^2.3.1",
    "prisma": "^2.25.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
