		"typescript": "5.3.2",
		"webpack": "5.89.0",
		"webpack-cli": "5.1.4"
	},
	"lint-staged": {
		"js/app/**/*.js": "eslint js/app/",
		"js/react/**/*.jsx": "eslint js/react/",
		"src/**/*.ts": "eslint src/",
		"views/**/*.pug": "pug-lint",
		"design/**/*.scss": "stylelint design/"
	}
