{
	"name": "donategifts",
	"version": "1.0.0",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"build": "npm run clean && npm run compile && npm run gulp:build && npm run webpack:build",
		"clean": "rimraf uploads dist public/css/*.css public/js/*.js",
		"compile": "tsc",
		"debug": "nodemon",
		"dev": "concurrently \"npm run debug\" \"npm run webpack:watch\" \"npm run gulp:watch\"",
		"docker:db": "docker-compose up mongo postgres",
		"docker": "docker-compose up",
		"gulp:build": "gulp",
		"gulp:watch": "gulp watch",
		"introspect": "kysely-codegen introspect --url postgres://postgres:postgres@localhost:5432/donategifts --out-file src/db/types/generated/database.ts",
		"lint:jsx": "eslint js/",
		"lint:pug": "pug-lint views",
		"lint:scss": "stylelint design/",
		"lint:src": "eslint src/",
		"lint": "npm run lint:pug && npm run lint:src && npm run lint:jsx && npm run lint:scss",
		"prepare": "husky install | chmod ug+x .husky/*",
		"start": "node dist/index.js",
		"webpack:build": "webpack --mode=production",
		"webpack:watch": "webpack --mode=development --watch",
		"seed:postgres": "node ./scripts/postgres-seeder/index.js",
		"seed:mongo": "node ./scripts/mongo-seeder/prepareDbExport.js && node ./scripts/mongo-seeder/seeder.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/donategifts/donategifts.git"
	},
	"author": "",
	"license": "NPOSL-3.0",
	"bugs": {
		"url": "https://github.com/donategifts/donategifts/issues"
	},
	"homepage": "https://github.com/donategifts/donategifts#readme",
	"dependencies": {
		"aws-sdk": "2.1501.0",
		"axios": "1.6.2",
		"bcryptjs": "2.4.3",
		"connect-mongo": "5.1.0",
		"connect-pg-simple": "9.0.0",
		"cookie-parser": "1.4.6",
		"cors": "2.8.5",
		"dotenv": "16.3.1",
		"express": "4.18.2",
		"express-rate-limit": "7.1.4",
		"express-session": "1.17.3",
		"express-validator": "7.0.1",
		"google-auth-library": "9.2.0",
		"jsonwebtoken": "9.0.2",
		"kysely": "0.26.3",
		"moment": "2.29.4",
		"mongoose": "6.8.1",
		"multer": "1.4.5-lts.1",
		"multer-sharp-s3": "0.2.5",
